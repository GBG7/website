{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "standard", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-use-before-define": "off",
    "no-unused-vars": "warn",
    "no-console": "error"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
